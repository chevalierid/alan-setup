.PARAM Delay = {MAX(Tdel,1n)}
.Param Rdel = {IF ( (Delay > 1E-15) & (Rout < 1), 1, Rout ) }
.Param VoutM={(VoutH+VoutL)/2}
.Param VthH={Vthres+Vhyst}
.Param VthL={Vthres-Vhyst}
.Param Cout={Delay/(0.693*(Rdel+1u))}
.Param Gdlh={1/Rdel}
