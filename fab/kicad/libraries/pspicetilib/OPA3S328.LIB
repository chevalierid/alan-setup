.SUBCKT OPA3S328_DECODER_2BITS IN0 IN1 REF OUT0 OUT1 OUT2
GO0 OUT0 REF VALUE={IF(V(IN1, REF) < 0.5 & V(IN0, REF) < 0.5, -1, 0)}
RO0 OUT0 REF R_NOISELESS 1
GO1 OUT1 REF VALUE={IF(V(IN1, REF) < 0.5 & V(IN0, REF) >= 0.5, -1, 0)}
RO1 OUT1 REF R_NOISELESS 1
GO2 OUT2 REF VALUE={IF(V(IN1, REF) >= 0.5 & V(IN0, REF) < 0.5, -1, 0)}
RO2 OUT2 REF R_NOISELESS 1
.ENDS OPA3S328_DECODER_2BITS
*$
*
.SUBCKT OPA3S328_DECODER_SHUTDOWN IN0 IN1 REF OUT4
GO4 OUT4 REF VALUE={IF(V(IN1, REF) >= 0.5 & V(IN0, REF) >= 0.5, 0, -1)}
RO4 OUT4 REF R_NOISELESS 1
.ENDS OPA3S328_DECODER_SHUTDOWN
*$
*
.SUBCKT OPA3S328_SCHMITT IN+ IN- OUT+ OUT- VCC VEE 
+ PARAMS:
+ VLOW = 0.3
+ VHI = 1.5
+ VOUT = 1
.PARAM VCC_MAX = 5.501
.PARAM VCC_MIN = 1.799
.PARAM VINT = 5
.PARAM R_R2 = {(VHI - VLOW) * 10000/VINT}
** Test for Min and Max power supplies
E1 N1 0 VALUE = {0.5*(SGN(VCC_MAX - V(VCC,VEE))+ ABS(SGN(VCC_MAX - V(VCC,VEE))))}
E2 N2 0 VALUE = {0.5*(SGN(V(VCC,VEE) - VCC_MIN)+ ABS(SGN(V(VCC,VEE) - VCC_MIN)))}
***
** Schmitt Trigger
G1 1 0 IN+ IN- -1
R1 1 0 R_NOISELESS 1 
R2 1 2 R_NOISELESS {R_R2}
V1 3 0 DC {(VHI * 10000)/(10000 + R_R2)}
G2 4 0 2 3 -1000
R4 4 0 R_NOISELESS 1
G3 6 0 VALUE={-LIMIT(V(4, 0), V(0), VINT)}
R5 6 0 R_NOISELESS 1
R3 6 2 R_NOISELESS 10k
C1 6 0 1p
G4 OUT+ OUT- VALUE={-VOUT * V(N1) * V(N2) * V(6, 0)/VINT}
R6 OUT+ OUT- R_NOISELESS 1
***
.ENDS OPA3S328_SCHMITT
