.SUBCKT OPA3S328_TON_TOFF IN+ IN- OUT+ OUT-
+ PARAMS:
+ VTH = 0.5
+ TON = 1.3e-6
+ TOFF = 2e-6
+ C1 = 1e-12

.PARAM R_R1 = {TOFF / (C1 * 0.69315)}
.PARAM R_R2 = {R_R1 * TON / (TOFF - TON)}
.PARAM V_VTH2 = {(R_R1 + 2.1*R_R2)/(2.1*(R_R1 + R_R2))}

G_IN 1 0 VALUE={IF(V(IN+, IN-) >= VTH, -1m, 1m)}
R_GIN 1 0 R_NOISELESS 1k
S_IN 3 4 1 0 GD_SWITCH

R1 3 0 R_NOISELESS {R_R1}
R2 4 0 R_NOISELESS {R_R2}
C1 3 0 {C1}
I_I1 0 3 {1/R_R1}

GD1 OUT+ OUT- VALUE={IF(V(3, 0) < V_VTH2, 1, 0)}
RD3 OUT+ OUT- R_NOISELESS 1

.MODEL GD_SWITCH VSWITCH ROFF=1G RON=1m VOFF=0.25 VON=0.75

.ENDS OPA3S328_TON_TOFF
