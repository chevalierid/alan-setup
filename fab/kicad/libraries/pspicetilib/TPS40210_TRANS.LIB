.model D_D1 d
+ is=1e-015
+ n=0.5
+ cjo=1e-012
+ tt=1e-011
*$
.subckt erramp in_p in_m  out  gnd params: isink= 1m isource=150u vhigh=8
+  vlow=100m pole=200 gain=5000
rin in_p in_m 8meg
cp1 11 gnd {1/(6.28*(gain/100u)*pole)}
e1 5 gnd 11 gnd 1
r9 5 2 5
d14 2 13 dmod
is 13 gnd {isink/100}
q1 gnd 13 16 qpmod
isrc 7 out {isource}
d12 out 7 dmod
d15 gnd 11 dclamp
g1 gnd 11 in_p in_m 100u
v1 7 gnd {vhigh-0.6v}
v4 out 16 {vlow-38mv}
rp1 11 gnd {gain/100u}
.model dclamp d
+ rs=10
+ bv=10
+ ibv=0.01
.model dmod d
+ tt=1e-009
+ cjo=1e-011
.ends erramp
